  "engines": {
    "node": ">=16.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ildella/ildella-test-commons.git"
  },
  "bugs": {
    "url": "https://github.com/ildella/ildella-test-commons/issues"
  },
  "homepage": "https://github.com/ildella/ildella-test-commons#readme",
  "author": {
    "name": "Daniele Dellafiore",
    "email": "daniele@dellafiore.net"
  },
  "main": "src/index",
  "scripts": {
    "cloc": "find src -name '*.js' | xargs wc -l",
    "cpd": "pmd cpd --minimum-tokens 16 --files src/,tests/ --language ecmascript --fail-on-violation false > cpd.report",
    "depcheck": "depcheck",
    "lint": "eslint . --max-warnings 3",
    "lint.print": "eslint --print-config src/config.js >> eslint-config.js",
    "test": "jest",
    "test.ci": "jest --ci",
    "test.coverage": "jest --coverage",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "axios": "1.1.3",
    "depcheck": "1.4.3",
    "eslint": "8.27.0",
    "eslint-config-node-moar": "0.8.1",
    "eslint-config-node-moar-stricter": "0.1.0",
    "eslint-config-node-moar-test": "0.1.3",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-jest": "27.1.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-plugin-unicorn": "44.0.2",
    "jest": "29.3.0",
    "node-notifier": "10.0.1",
    "through2": "4.0.2"
  },
  "devDependencies": {
    "aws-sdk": "2.1245.0",
    "dayjs": "1.11.6",
    "exstream.js": "0.22.0",
    "fastify": "4.9.2",
    "find-versions-cli": "4.0.0",
    "highland": "2.13.5",
    "jest-dynalite": "3.6.1",
    "kinesalite": "3.3.3",
    "mongodb": "4.11.0",
    "mongodb-memory-server-core": "8.9.5",
    "stream-mock": "2.0.5",
    "tracer": "1.1.6"
  },
  "peerDependencies": {
    "aws-sdk": "^2",
    "dayjs": "^1",
    "fastify": "^4",
    "highland": "^2",
    "jest-dynalite": "^3",
    "kinesalite": "^3",
    "mongodb": "^4",
    "mongodb-memory-server-core": "^8",
    "tracer": "^1"
  }
}
